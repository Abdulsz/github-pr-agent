{
  "name": "github-pr-agent",
  "version": "1.0.0",
  "description": "AI-powered GitHub PR Agent built on Cloudflare",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "wrangler dev --port 8787",
    "build": "vite build",
    "deploy": "npm run build && wrangler deploy",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cloudflare",
    "agents",
    "github",
    "ai",
    "pr"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^3.0.26",
    "@cloudflare/workers-types": "^4.20260210.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@vitejs/plugin-react": "^4.7.0",
    "agents": "^0.4.0",
    "ai": "^6.0.78",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "workers-ai-provider": "^0.1.3",
    "wrangler": "^4.64.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1"
  }
}
